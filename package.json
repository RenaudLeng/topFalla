{
  "name": "topfalla",
  "version": "1.0.0",
  "description": "Comparateur de prix pour le Gabon - TopFalla",
  "main": "server.js",
  "scripts": {
    "start": "set NODE_ENV=production && node app.js",
    "dev": "set NODE_ENV=development && nodemon app.js",
    "test:win": "set NODE_ENV=test && jest --detectOpenHandles",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "build": "echo 'Build step non n√©cessaire pour cette version de base'",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "format": "prettier --write \"**/*.{js,jsx,json,md,html,css}\"",
    "lint": "eslint . --ext .js",
    "preview": "npx serve -s public",
    "prepare": "husky install",
    "db:create": "sequelize-cli db:create",
    "db:drop": "sequelize-cli db:drop",
    "db:reset": "npm run db:drop && npm run db:create && npm run migrate && npm run seed"
  },
  "keywords": [
    "comparateur",
    "prix",
    "gabon",
    "pwa",
    "topfalla",
    "ecommerce",
    "comparateur-de-prix"
  ],
  "author": "ML.Inc",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.0",
    "chart.js": "^4.4.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^3.1.9",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "http-status": "^1.8.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.13.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.9",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.3",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "serve": "^14.2.0",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "xss-clean": "^0.1.4",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}
